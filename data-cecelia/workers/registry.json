{
  "$schema": "./registry.schema.json",
  "version": "1.0.0",
  "description": "AI 员工注册表 - 业务到 Runner 的映射",
  "n8n": {
    "baseUrl": "http://localhost:5679",
    "webhookBase": "http://localhost:5679/webhook"
  },
  "features": {
    "devops": {
      "name": "DevOps",
      "description": "开发运维",
      "businesses": {
        "devgate.push": {
          "name": "DevGate 指标推送",
          "type": "L1",
          "owner": "spark",
          "runners": [
            {
              "type": "n8n",
              "workflow": "DevGate Nightly Push",
              "workflowId": "7vQUOfubH4vp8CiD",
              "webhook": "/webhook/devgate-push",
              "method": "POST"
            },
            {
              "type": "script",
              "path": "zenithjoy-engine/scripts/devgate/push-metrics.sh",
              "env": ["DEVGATE_API_TOKEN"]
            }
          ]
        },
        "cecelia.execute": {
          "name": "无头 Claude 执行",
          "type": "L2",
          "owner": "spark",
          "runners": [
            {
              "type": "n8n",
              "workflow": "Trigger Claude via HTTP",
              "workflowId": "efoglxmVKtH3f72i",
              "webhook": "/webhook/trigger-claude",
              "method": "POST"
            }
          ]
        },
        "cecelia.callback": {
          "name": "执行回调处理",
          "type": "L2",
          "owner": "echo",
          "runners": [
            {
              "type": "n8n",
              "workflow": "cecelia-callback-simple",
              "workflowId": "cb210"
            }
          ]
        },
        "task.dispatch": {
          "name": "任务调度分发",
          "type": "L3",
          "owner": "echo",
          "dependsOn": ["cecelia.execute", "cecelia.callback"],
          "runners": [
            {
              "type": "n8n",
              "workflow": "Task Dispatcher v2.0",
              "workflowId": "FkD3AmxALsv6Y3Pv"
            }
          ]
        }
      }
    },
    "dataops": {
      "name": "DataOps",
      "description": "数据采集",
      "businesses": {
        "scrape.douyin": {
