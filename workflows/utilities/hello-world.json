{
  "name": "Hello World - First Workflow",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "minutes",
              "minutesInterval": 5
            }
          ]
        }
      },
      "id": "schedule-trigger-hello",
      "name": "每5分钟触发",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.2,
      "position": [
        250,
        300
      ]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "message-id",
              "name": "message",
              "value": "Hello World! 当前时间: {{ $now.format(\"yyyy-MM-dd HH:mm:ss\") }}",
              "type": "string"
            },
            {
              "id": "count-id",
              "name": "executionCount",
              "value": "={{ $execution.id }}",
              "type": "number"
            }
          ]
        }
      },
      "id": "set-data",
      "name": "设置数据",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.4,
      "position": [
        450,
        300
      ]
    },
    {
      "parameters": {
        "operation": "formatDate",
        "date": "={{ $now }}",
        "format": "yyyy-MM-dd HH:mm:ss",
        "options": {}
      },
      "id": "format-date",
      "name": "格式化时间",
      "type": "n8n-nodes-base.dateTime",
      "typeVersion": 2,
      "position": [
        650,
        300
      ]
    }
  ],
  "connections": {
    "每5分钟触发": {
      "main": [
        [
          {
            "node": "设置数据",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "设置数据": {
      "main": [
        [
          {
            "node": "格式化时间",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "settings": {
    "executionOrder": "v1",
    "callerPolicy": "workflowsFromSameOwner",
    "availableInMCP": true
  },
  "active": false
}
